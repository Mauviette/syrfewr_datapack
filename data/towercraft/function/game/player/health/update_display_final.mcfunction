$execute if score @p[tag=tc.playing] s.CoreHealth matches 51.. run data modify entity @n[tag=tc.display_core] text set value '{"text":"$(health)%","color":"green"}'
$execute if score @p[tag=tc.playing] s.CoreHealth matches 26..50 run data modify entity @n[tag=tc.display_core] text set value '{"text":"$(health)%","color":"yellow"}'
$execute if score @p[tag=tc.playing] s.CoreHealth matches ..25 run data modify entity @n[tag=tc.display_core] text set value '{"text":"$(health)%","color":"red"}'