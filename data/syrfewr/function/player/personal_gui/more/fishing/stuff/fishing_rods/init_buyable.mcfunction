#Par défaut
execute unless score @s s.SelectedFishingRodSkinID matches 0 run item replace entity @s inventory.1 with fishing_rod[custom_data={ui:1,to_kill:1,select:1},item_name='{"color":"white","text":"Par défaut"}',lore=['{"color":"gray","text":"Aucune particularité","italic":false}']]
execute if score @s s.SelectedFishingRodSkinID matches 0 run item replace entity @s inventory.1 with fishing_rod[custom_data={ui:1,to_kill:1,select:1},item_name='{"color":"white","text":"Par défaut"}',lore=['{"color":"dark_green","text":"Sélectionné","italic":false}','{"color":"dark_gray","text":"Aucune particularité","italic":false}'],enchantments={levels:{"minecraft:protection":1},show_in_tooltip:false}]

